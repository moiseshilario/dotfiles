{
  "editor.cursorBlinking": "phase",
  "editor.fontFamily": "Source Code Pro, Menlo, Fira Code iScript,Space Mono, JetBrains Mono, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontWeight": "400",
  "editor.minimap.showSlider": "always",
  "editor.parameterHints.cycle": true,
  "editor.renderWhitespace": "boundary",
  "editor.smoothScrolling": true,
  "editor.stablePeek": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.semanticHighlighting.enabled": true,
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 1,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.fontAliasing": "auto",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.horizontalScrolling": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "nunjucks": "html",
    "javascript": "javascriptreact"
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.next": true,
    "**/.svn": true,
    "**/bower_components": true,
    "**/build": true,
    "**/CVS": true,
    "**/dist": false,
    "**/.vscode": true,
    // "**/node_modules": true
  },
  "files.associations": {
    ".ackrc": "shellscript",
    ".aliases": "shellscript",
    ".bash_profile": "shellscript",
    ".bash_prompt": "shellscript",
    ".bashrc": "shellscript",
    ".bootstrap": "shellscript",
    ".brew": "shellscript",
    ".config": "shellscript",
    ".curlrc": "shellscript",
    ".exports": "shellscript",
    ".extra": "shellscript",
    ".functions": "shellscript",
    ".git": "shellscript",
    ".gitattributes": "shellscript",
    ".gitconfig": "shellscript",
    ".githelpers": "shellscript",
    ".gitignore_global": "shellscript",
    ".gitignore": "shellscript",
    ".gitmessage": "shellscript",
    ".hushlogin": "shellscript",
    ".inputrc": "shellscript",
    ".npmrc": "shellscript",
    ".osx": "shellscript",
    ".packages": "shellscript",
    ".pkginit": "shellscript",
    ".screenrc": "shellscript",
    ".symlink": "shellscript",
    ".vim": "shellscript",
    ".vimrc": "shellscript",
    ".wgetrc": "shellscript",
    ".xsessionrc": "shellscript",
    ".zlogin": "shellscript",
    ".zlogout": "shellscript",
    ".zpreztorc": "shellscript",
    ".zprofile": "shellscript",
    ".zshenv": "shellscript",
    ".zshrc": "shellscript",
    "*.html": "html",
    "*.js": "javascript",
    "bash": "shellscript",
    "Berkfile": "shellscript",
    "Cheffile": "shellscript",
    "Gemfile": "shellscript",
    "Rakefile": "shellscript",
    "sh": "shellscript",
    "symlink": "shellscript",
    "Vagrantfile": "shellscript",
    "zlogin": "shellscript",
    "zlogout": "shellscript",
    "zpreztorc": "shellscript",
    "zprofile": "shellscript",
    "zsh": "shellscript",
    "zshenv": "shellscript",
    "zshrc": "shellscript",
    "Fastfile": "ruby"
  },
  "git.openDiffOnClick": false,
  "git.enableSmartCommit": true,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "terminal.integrated.cursorBlinking": true,
  "html.format.indentInnerHtml": true,
  "css.validate": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "extensions.ignoreRecommendations": false,
  // Theme specifics
  "oneDarkPro.bold": true,
  "emmet.showSuggestionsAsSnippets": false,
  "prettier.arrowParens": "always",
  "prettier.jsxSingleQuote": true,
  "prettier.printWidth": 100,
  "prettier.singleQuote": true,
  "prettier.requireConfig": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnPaste": true,
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "turboConsoleLog.quote": "'",
  "editor.parameterHints.enabled": false,
  "typescript.format.enable": false,
  "editor.formatOnSave": true,
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "todohighlight.isEnable": false,
  "git.confirmSync": false,
  "helium-icon-theme.folders.color": "#90a4ae",
  "material-icon-theme.activeIconPack": "react",
  "editor.tokenColorCustomizations": {
    "[Omni]": {
      "textMateRules": [
        {
          "scope": [
            "storage"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "entity.name.function"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        }
      ]
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "editor.accessibilitySupport": "off",
  "react-native-tools.showUserTips": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.suggest.preview": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "security.workspace.trust.untrustedFiles": "open",
  "dart.openDevTools": "flutter",
  "workbench.colorTheme": "Omni",
  "dart.flutterRunAdditionalArgs": [
    "--no-sound-null-safety"
  ]
}
